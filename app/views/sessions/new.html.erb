<h1>Log In</h1>

<% if flash[:notice] %>
	<p><%= flash[:notice]  %></p>
<% end %>
<% if flash[:alert] %>
	<p><%= flash[:alert]  %></p>
<% end %>

<%= form_tag sessions_path do  %>

<%= label_tag :email  %>
<%= text_field_tag :email, params[:email]  %>

<%= label_tag :password  %>
<%= password_field_tag(:pasword)  %>

<%= submit_tag "Login" %>

<% end %>